{
  "name": "remix-tailwind-starter-kit",
  "description": "Remix starter kit with Tailwind CSS family of libraries",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@headlessui/react": "1.5.0",
    "@heroicons/react": "1.0.6",
    "@radix-ui/react-context-menu": "0.1.6",
    "@radix-ui/react-navigation-menu": "0.1.2",
    "@radix-ui/react-scroll-area": "0.1.4",
    "@radix-ui/react-tabs": "0.1.5",
    "@remix-run/react": "1.3.5",
    "@remix-run/vercel": "1.3.5",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "@tailwindcss/typography": "0.5.2",
    "@tanem/react-nprogress": "4.0.13",
    "@vechaiui/core": "0.6.0",
    "@vechaiui/react": "0.2.2",
    "clsx": "1.1.1",
    "daisyui": "2.13.6",
    "graphql": "16.3.0",
    "graphql-request": "4.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.29.0",
    "remix": "1.3.5",
    "tailwindcss": "3.0.23",
    "tailwindcss-radix": "2.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.3.5",
    "@remix-run/eslint-config": "1.3.5",
    "@remix-run/serve": "1.3.5",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "autoprefixer": "10.4.4",
    "concurrently": "7.1.0",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.8",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  }
}
